func void AMBIENT_ASKWHOYOUARE() {
    B_STANDUP();
    if ((OTHER.GUILD) == (GIL_MIL)) {
        if (((MARVIN_MILITIASPECIALIZATION) == (MILITIA_SPECIALIZATION_FIGHTER)) || ((MARVIN_MILITIASPECIALIZATION) == (MILITIA_SPECIALIZATION_MASTERFIGHTER))) {
            B_SAY(SELF, OTHER, "$GUILDAMBIENT_Militia_WhoYouAre_V1");
        } else if (((MARVIN_MILITIASPECIALIZATION) == (MILITIA_SPECIALIZATION_CROSSBOWMAN)) || ((MARVIN_MILITIASPECIALIZATION) == (MILITIA_SPECIALIZATION_MASTERCROSSBOWMAN))) {
            B_SAY(SELF, OTHER, "$GUILDAMBIENT_Militia_WhoYouAre_V2");
        } else {
            B_SAY(SELF, OTHER, "$GUILDAMBIENT_WhoYouAre_V1");
        } else {
            /* set_instance(0) */;
        };
    };
    if ((OTHER.GUILD) == (GIL_SLD)) {
        if (((MARVIN_ARAXOSSPECIALIZATION) == (ARAXOS_SPECIALIZATION_FIGHTER)) || ((MARVIN_ARAXOSSPECIALIZATION) == (ARAXOS_SPECIALIZATION_MASTERFIGHTER))) {
            B_SAY(SELF, OTHER, "$GUILDAMBIENT_Araxos_WhoYouAre_V1");
        } else if (((MARVIN_ARAXOSSPECIALIZATION) == (ARAXOS_SPECIALIZATION_ARCHER)) || ((MARVIN_ARAXOSSPECIALIZATION) == (ARAXOS_SPECIALIZATION_MASTERARCHER))) {
            B_SAY(SELF, OTHER, "$GUILDAMBIENT_Araxos_WhoYouAre_V2");
        } else {
            B_SAY(SELF, OTHER, "$GUILDAMBIENT_WhoYouAre_V2");
        } else {
            /* set_instance(0) */;
        };
    };
    B_SAY(SELF, OTHER, "$GUILDAMBIENT_WhoYouAre_V3");
    B_SAY(OTHER, SELF, "$GUILDAMBIENT_WhoYouAre_Yes");
}

