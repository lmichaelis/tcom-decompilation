var int MUSIC_CURRENTOVERRIDE = 0;
func void MUSIC_OVERRIDETRACK(var int TRACK) {
    _KMLIB_MUSICSYSTEM_OVERRIDETRACK_PTR = KMLIB_GETPROCADDRESS("MusicSystem_OverrideTrack");
    MUSIC_CURRENTOVERRIDE = TRACK;
    PRINTD(CS2("Music override: ", _PM_INSTNAME(TRACK)));
    CALL_INTPARAM(TRACK);
    CALL__CDECL(_KMLIB_MUSICSYSTEM_OVERRIDETRACK_PTR);
}

const int MUSIC_OVERRIDETRACK._KMLIB_MUSICSYSTEM_OVERRIDETRACK_PTR = 0;
func void MUSIC_DISABLEOVERRIDE() {
    _KMLIB_MUSICSYSTEM_DISABLETRACKOVERRIDE_PTR = KMLIB_GETPROCADDRESS("MusicSystem_DisableTrackOverride");
    PRINTD("Music override disabled");
    MUSIC_CURRENTOVERRIDE = 0;
    CALL__CDECL(_KMLIB_MUSICSYSTEM_DISABLETRACKOVERRIDE_PTR);
}

const int MUSIC_DISABLEOVERRIDE._KMLIB_MUSICSYSTEM_DISABLETRACKOVERRIDE_PTR = 0;
func void INIT_MUSICSYSTEM_ALWAYS() {
    if ((MUSIC_CURRENTOVERRIDE) > (0)) {
        MUSIC_OVERRIDETRACK(MUSIC_CURRENTOVERRIDE);
    };
    MUSIC_DISABLEOVERRIDE();
}

