func void B_ASSIGNAMBIENTINFOS(var C_NPC SLF, var int INASSESSPLAYER) {
    if ((SLF.NPCTYPE) == (NPCTYPE_AMBIENT)) {
        if ((SLF.GUILD) == (GIL_MIL)) {
            if (C_CITY_IAMOLDCITYGUARD(SLF)) {
                B_ASSIGNAMBIENTINFOS_MIL_OLDCITYGUIDE(SLF);
            } else {
                B_ASSIGNAMBIENTINFOS_MIL_CITY(SLF);
            } else {
                /* set_instance(0) */;
            };
        };
        if ((SLF.GUILD) == (GIL_SLD)) {
            if (C_FASTTRAVEL_IAMARAXOS(SLF)) {
                B_ASSIGNAMBIENTINFOS_SLD_FASTRAVEL(SLF);
            } else if (C_FASTTRAVEL_IAMCITYGUIDE(SLF)) {
                B_ASSIGNAMBIENTINFOS_SLD_CITYGUIDE(SLF);
            } else if ((C_ARAXOSMINE_IMARAXOS_INSIDE(SLF)) == (FALSE)) {
                B_ASSIGNAMBIENTINFOS_SLD_CITY(SLF);
            };
        } else if ((SLF.GUILD) == (GIL_NONE)) {
            if (C_CITY_IAMSLUMSGUARDIAN(SLF)) {
                B_ASSIGNAMBIENTINFOS_SLUMSGUARDIAN(SLF);
            } else if (C_IAMSALVISPY(SLF)) {
                B_ASSIGNAMBIENTINFOS_SALVISPY(SLF);
            } else if (C_IAMGETORYKSPY(SLF)) {
                B_ASSIGNAMBIENTINFOS_GETORYKSPY(SLF);
            };
        };
    };
    if ((C_CITY_IAMFROMOLDCITY(SLF)) == (TRUE)) {
        B_ASSIGNAMBIENTINFOS_SQ225_OLDCITY(SLF);
    };
    if ((C_MILITIAMINE_IAMMILITIA(SLF)) == (TRUE)) {
        B_ASSIGNAMBIENTINFOS_SQ311_MILITIA(SLF);
    };
    if ((C_VINEYARD_IAMFROMVALERIO(SLF)) == (TRUE)) {
        B_ASSIGNAMBIENTINFOS_BAU_VALERIOVINE(SLF);
    };
    if ((C_Q206_GUARDFINBARBODY(SLF)) == (TRUE)) {
        B_ASSIGNAMBIENTINFOS_Q206_FINBARBODYGUARD(SLF);
    };
    if ((C_SQ505_IMCITIZEN_M7(SLF)) == (TRUE)) {
        B_ASSIGNAMBIENTINFOS_SQ505_CITIZEN_M7(SLF);
    };
    if ((C_SQ505_IMVOLKERGUARD_M3(SLF)) == (TRUE)) {
        B_ASSIGNAMBIENTINFOS_SQ505_VOLKERGUARD_M3(SLF);
    };
    if ((C_ARAXOSMINE_IMARAXOS_INSIDE(SLF)) == (TRUE)) {
        B_ASSIGNAMBIENTINFOS_SLD_ARAXOSMINE(SLF);
    };
    if (!(INASSESSPLAYER)) {
        if ((SLF.NPCTYPE) == (NPCTYPE_AMBIENT)) {
            if ((CURRENTLEVEL) == (ARCHOLOS_VOLFZACKE_ZEN)) {
                if ((C_VOLFZACK_IAMWARRIOR(SLF)) == (TRUE)) {
                    B_ASSIGNAMBIENTINFOS_VOLFZACKWARRIOR(SLF);
                };
            } else if ((SLF.GUILD) == (GIL_VLK)) {
                B_ASSIGNAMBIENTINFOS_VLK_CITY(SLF);
            } else if ((SLF.GUILD) == (GIL_DJG)) {
                B_ASSIGNAMBIENTINFOS_DJG(SLF);
            };
        };
        if ((C_AMBIENT_SILBACH(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_SILBACH(SLF);
        } else if ((C_AMBIENT_VALERIO(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_VALERIO(SLF);
        } else if ((C_AMBIENT_RITA(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_RITA(SLF);
        } else if ((C_AMBIENT_BERMAR(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_BERMAR(SLF);
        } else if ((C_AMBIENT_FARMERSCITY(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_FARMERCITY(SLF);
        } else if ((C_AMBIENT_FARMER_WOLFSDEN(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_FARMERWOLFSDEN(SLF);
        } else if ((C_AMBIENT_FISHERMANS(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_FISHERMANS(SLF);
        } else if ((C_AMBIENT_PIRATES(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_HAVEN(SLF);
        } else if ((C_AMBIENT_WOLFSON(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_WOLFSDEN(SLF);
        } else if ((C_AMBIENT_CITY_ARAXOS(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_CITY_SLD(SLF);
        } else if ((C_AMBIENT_MINE_ARAXOS(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_MINE_SLD(SLF);
        } else if ((C_AMBIENT_CITY_MILITIA(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_CITY_MIL(SLF);
        } else if ((C_AMBIENT_MINE_MILITIA(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_MINE_MIL(SLF);
        } else if ((C_AMBIENT_CITY_CITIZEN(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_CITY_CITIZEN(SLF);
        } else if ((C_AMBIENT_CITY_SLUMS(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_CITY_SLUMS(SLF);
        } else if ((C_AMBIENT_CITY_OLDCITY(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_CITY_OLDCITY(SLF);
        } else if ((C_AMBIENT_CITY_HOMELESS(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_CITY_HOMELESS(SLF);
        } else if ((C_AMBIENT_CITY_SEWERS(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_CITY_SEWERS(SLF);
        } else if ((C_AMBIENT_REFUGEE(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_REFUGEE(SLF);
        } else if ((C_AMBIENT_NOVIZE(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_NOVIZE(SLF);
        } else if ((C_AMBIENT_VOLKER(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_USURER(SLF);
        } else if ((C_AMBIENT_WOODCUTTER(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_WOODCUTTER(SLF);
        } else if ((C_AMBIENT_COALS(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_COALS(SLF);
        } else if ((C_AMBIENT_ROYALGUARD(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_ROYALGUARD(SLF);
        } else if ((C_AMBIENT_CITY_ARAXOSGUIDE(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_CITY_GUIDE(SLF);
        } else if ((C_AMBIENT_MINE_MINER_ARAXOS(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_MINER_ARX(SLF);
        } else if ((C_AMBIENT_MINE_MINER_MILITIA(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_MINER_KING(SLF);
        } else if ((C_AMBIENT_CITY_SOUTHERNER(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_CITY_SOUTHERNER(SLF);
        } else if ((C_AMBIENT_CITY_WORKER(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_CITY_WORKERS(SLF);
        } else if ((C_AMBIENT_CITY_SAILOR(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_SAILORS(SLF);
        } else if ((C_AMBIENT_BOAT_FASTTRAVEL(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_AMB_FASTTRAVELBOAT(SLF);
        };
        if ((C_FASTTRAVEL_IAMFISHERMAN(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_BAU_FASTTRAVEL(SLF);
        };
        if ((C_VINEYARD_IAMFROMGASPAR(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_BAU_GASPARVINE(SLF);
        } else if ((C_SILBACH_IAMFARMER(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_BAU_SILBACH(SLF);
        } else if ((C_QM401_RENEGADEATTACKGROUP(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_QM401_RENEGADEATTACKGROUP(SLF);
        } else if ((C_PART1_IAMFISHERMAN(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_PART1_FISHERMAN(SLF);
        } else if ((C_PART4_IAMFISHERMAN(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_PART4_FISHERMAN(SLF);
        } else if ((C_CITY_IAMREFUGEEMATEY(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_VLK_MATEYREFUGEE(SLF);
        } else if ((C_ARAXOSMINE_IAMMINER(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_NONE_ARAXOSWORKERS(SLF);
        } else if ((C_Q308_IAMREFUGEE(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_Q308_REFUGEE(SLF);
        } else if ((C_Q308_IAMHELPINGREFUGEE(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_Q308_REFUGEEHELPERS(SLF);
        } else if ((C_Q308_IAMCITIZENVOLKER(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_Q308_CITIZENVOLKER(SLF);
        } else if ((C_CITY_IMFROMSLUMS(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_VLK_SLUMS(SLF);
        } else if ((C_KQ407_ARMY(SLF)) == (TRUE)) {
            B_ASSIGNAMBIENTINFOS_KQ407_ARMY(SLF);
        } else if (C_IAMMANNEQUIN(SLF)) {
            B_ASSIGNAMBIENTINFOS_MANNEQUIN(SLF);
        } else if (C_ARAXOSMINE_IAMNAARSMINER(SLF)) {
            B_ASSIGNAMBIENTINFOS_NONE_NAARSWORKERS(SLF);
        } else if (C_Q308_IAMMAGE(SLF)) {
            B_ASSIGNAMBIENTINFOS_Q308_MAGES(SLF);
        } else if (C_IAMMUSICIAN(SLF)) {
            B_ASSIGNAMBIENTINFOS_MUSICIAN(SLF);
        } else if (C_Q401_IAMWORKER(SLF)) {
            B_ASSIGNAMBIENTINFOS_Q401_WORKERS(SLF);
        } else if (C_IAMBUSY_CALM(SLF)) {
            B_ASSIGNAMBIENTINFOS_AMBIENT_IMBUSY(SLF);
        } else if (C_MILITIAMINE_IAMMINER(SLF)) {
            B_ASSIGNAMBIENTINFOS_BAU_MILITIAMINE(SLF);
        } else if (C_SQ225_ASKABOUTGERHARD(SLF)) {
            B_ASSIGNAMBIENTINFOS_SQ225_ABOUTGERHARD(SLF);
        };
    };
}

