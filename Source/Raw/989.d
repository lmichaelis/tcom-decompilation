func void B_SETVISUALS_DEMON_BUG() {
    MDL_SETVISUAL(SELF, "Demon_Bug.mds");
    MDL_SETVISUALBODY(SELF, "Giant_dem_bug_body", DEFAULT, DEFAULT, "", DEFAULT, DEFAULT, -(1));
}

func void B_SETVISUALS_DARKDEMON_BUG() {
    MDL_SETVISUAL(SELF, "Demon_Bug.mds");
