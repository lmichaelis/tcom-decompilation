func void DIA_FABIO_Q602_HELLO_INFO() {
    if (HLP_ISVALIDNPC(NONE_6364_SIGMUN_Q602)) {
        if (!(NPC_ISDEAD(NONE_6364_SIGMUN_Q602))) {
            TRIA_INVITE(NONE_6364_SIGMUN_Q602);
            TRIA_START();
        };
    };
    AI_OUTPUT(SELF, OTHER, "DIA_Fabio_Q602_HELLO_05_01");
    AI_OUTPUT(SELF, OTHER, "DIA_Fabio_Q602_HELLO_05_02");
    AI_LOGENTRY(TOPIC_Q602, LOG_Q602_FABIO_AGAINMEETING);
    INFO_CLEARCHOICES(0x161e4);
    INFO_ADDCHOICE(0x161e4, "He's dead.", 0x161e9);
    INFO_ADDCHOICE(0x161e4, "Shut up, Fabio.", 0x161e8);
}

