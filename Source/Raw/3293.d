func void B_CLEARFAKEITEMS(var C_NPC SLF) {
    if (HLP_ISVALIDNPC(SLF)) {
        PRINTD("Usuwam EQ");
        NPC_REMOVEINVITEMS(SLF, 0x8d23, NPC_HASITEMS(SLF, 0x8d23));
        NPC_REMOVEINVITEMS(SLF, 0x9abd, NPC_HASITEMS(SLF, 0x9abd));
    };
}

func void B_CLEARJUNKTRADEINV(var C_NPC SLF) {
    if (HLP_ISVALIDNPC(SLF)) {
        NPC_REMOVEINVITEMS(SLF, 0x864f, NPC_HASITEMS(SLF, 0x864f));
        NPC_REMOVEINVITEMS(SLF, 0x8650, NPC_HASITEMS(SLF, 0x8650));
    };
}

