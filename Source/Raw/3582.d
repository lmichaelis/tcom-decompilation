func void B_ENTER_ARCHOLOS_ENDGAME() {
    if ((Q602_STARTQUEST) == (FALSE)) {
        Q602_STARTQUEST = 1;
        WLD_SETTIME(19, 20);
        if ((LOG_GETSTATUS(MIS_Q601)) == (LOG_RUNNING)) {
            LOG_SETSTATUS(_@(MIS_Q601), TOPIC_Q601, LOG_SUCCESS);
            B_LOGENTRY(TOPIC_Q601, LOG_Q601_FINISH);
        };
        LOG_CREATETOPIC(TOPIC_Q602, LOG_MISSION);
        LOG_SETSTATUS(_@(MIS_Q602), TOPIC_Q602, LOG_RUNNING);
        B_LOGENTRY(TOPIC_Q602, LOG_Q602_START);
        FADESCREENFROMBLACK(1);
    };
}

var int B_ENTER_ARCHOLOS_ENDGAME.Q602_STARTQUEST = 0;
