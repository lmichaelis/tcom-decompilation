        WLD_SENDTRIGGER("MOVER_SQ311_MALEN");
        SQ311_MALENDEADSCENE_COLLISION = 1;
    };
    if (((MOVER1.MOVERSTATE) != (MOVER_STATE_OPENING)) && ((MOVER1.MOVERSTATE) != (MOVER_STATE_CLOSING))) {
