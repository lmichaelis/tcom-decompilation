func void B_REMOVENPC(var int NPCINSTANCE) {
    NPC = HLP_GETNPC(NPCINSTANCE);
    if (HLP_ISVALIDNPC(NPC)) {
        NPC.FLAGS = 0;
        B_STARTOTHERROUTINE(NPC, TOT);
        NPC_CHANGEATTRIBUTE(NPC, ATR_HITPOINTS, -(NPC.ATTRIBUTE[1]));
        TELEPORTNPCTOWP(NPCINSTANCE, TOT);
    };
}

