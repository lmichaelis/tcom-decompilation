func void ZS_EXECUTION() {
    PERCEPTION_SET_NONE();
    SELF.NOFOCUS = TRUE;
    SELF.AIVAR[15] = TRUE;
    B_RESETALL(SELF);
    if (!(C_BODYSTATECONTAINS(SELF, BS_SIT))) {
        AI_SETWALKMODE(SELF, NPC_WALK);
        if ((HLP_STRCMP(NPC_GETNEARESTWP(SELF), SELF.WP)) == (FALSE)) {
            AI_GOTOWP(SELF, SELF.WP);
        };
    };
}

func int ZS_EXECUTION_LOOP() {
    if ((!(C_BODYSTATECONTAINS(SELF, BS_SIT))) && (WLD_ISMOBAVAILABLE(SELF, "EXECUTIONKNEEL"))) {
        AI_USEMOB(SELF, "EXECUTIONKNEEL", 1);
    };
    return LOOP_CONTINUE;
}

func void ZS_EXECUTION_END() {
    AI_USEMOB(SELF, "EXECUTIONKNEEL", -(1));
}

