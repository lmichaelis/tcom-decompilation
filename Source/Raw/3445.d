func void GQ001_REMOVEFARMERS() {
    if (NPC_KNOWSINFO(HERO, 0x155e6)) {
        B_STARTOTHERROUTINE(BAU_11010_FARMER, START);
        NPC_REFRESH(BAU_11010_FARMER);
    };
    B_STARTOTHERROUTINE(BAU_11010_FARMER, "PRESTART");
    NPC_REFRESH(BAU_11010_FARMER);
    B_STARTOTHERROUTINE(BAU_11012_FARMER, START);
    NPC_REFRESH(BAU_11012_FARMER);
    B_STARTOTHERROUTINE(BAU_11026_FARMER, START);
    NPC_REFRESH(BAU_11026_FARMER);
    B_STARTOTHERROUTINE(BAU_714_REMY, START);
    NPC_REFRESH(BAU_714_REMY);
    B_STARTOTHERROUTINE(BAU_11031_FARMER, START);
    NPC_REFRESH(BAU_11031_FARMER);
    if ((SQ103_GOWITHRUPERT) == (1)) {
        if ((LOG_GETSTATUS(MIS_SQ103)) == (LOG_SUCCESS)) {
            if ((Q104_RUPERTBROTHERLYWORK) == (TRUE)) {
                B_STARTOTHERROUTINE(NONE_100007_RUPERT, "Work");
                NPC_REFRESH(NONE_100007_RUPERT);
            } else {
                B_STARTOTHERROUTINE(NONE_100007_RUPERT, PUB01);
                NPC_REFRESH(NONE_100007_RUPERT);
            };
        };
    };
    B_STARTOTHERROUTINE(NONE_100007_RUPERT, PUB01);
    NPC_REFRESH(NONE_100007_RUPERT);
    B_STARTOTHERROUTINE(BAU_701_KURT, "TalkWithAlbyn");
    NPC_REFRESH(BAU_701_KURT);
    B_STARTOTHERROUTINE(BAU_700_ALBYN, "TalkWithKurt");
    NPC_REFRESH(BAU_700_ALBYN);
}

func void GQ001_STARTENTERCITYQUEST() {
    if ((LOG_GETSTATUS(MIS_GQ001)) == (LOG_RUNNING)) {
        if ((GQ001_TELEPORTALBYNANDKURT) == (0)) {
            GQ001_TELEPORTALBYNANDKURT = 1;
            if ((GQ001_KURTFOUNDRING) == (FALSE)) {
                WLD_SENDTRIGGER("GQ001_DIRT_01");
            };
            if ((GQ001_KURTFOUNDPOCKET01) == (FALSE)) {
                WLD_SENDTRIGGER("GQ001_DIRT_02");
            };
            if ((GQ001_KURTFOUNDPOCKET02) == (FALSE)) {
                WLD_SENDTRIGGER("GQ001_DIRT_03");
            };
            LOG_CREATETOPIC(TOPIC_Q201, LOG_MISSION);
            LOG_SETSTATUS(_@(MIS_Q201), TOPIC_Q201, LOG_RUNNING);
            AI_LOGENTRY(TOPIC_Q201, LOG_Q201_START);
            LOG_SETSTATUS(_@(MIS_GQ001), TOPIC_GQ001, LOG_SUCCESS);
            AI_LOGENTRY(TOPIC_GQ001, LOG_Q201_START);
            B_GIVEPLAYERXP(XP_GQ001_FINISH);
            B_STARTOTHERROUTINE(BAU_701_KURT, "Tired");
            NPC_REFRESH(BAU_701_KURT);
            B_STARTOTHERROUTINE(BAU_700_ALBYN, START);
            NPC_REFRESH(BAU_700_ALBYN);
        };
    };
}

