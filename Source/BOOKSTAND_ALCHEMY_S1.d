func void BOOKSTAND_ALCHEMY_S1() {
    var int NDOCID;
    var int BOOKSTAND_ALCHEMY_CHECKREAD;
    var C_NPC HER;
    HER = HLP_GETNPC(50091);
    if ((HLP_GETINSTANCEID(SELF)) == (HLP_GETINSTANCEID(HER))) {
        if ((BOOKSTAND_ALCHEMY_CHECKREAD) == (FALSE)) {
            BOOKSTAND_ALCHEMY_CHECKREAD = TRUE;
            B_GIVEPLAYERXP(XP_READBOOKSTAND);
        };
        NDOCID = DOC_CREATE();
        DOC_SETPAGES(NDOCID, 2);
        DOC_SETPAGE(NDOCID, 0, "Book_KM_01_L.tga", 0);
        DOC_SETPAGE(NDOCID, 1, "Book_KM_01_R.tga", 0);
        DOC_SETFONT(NDOCID, -(1), FONT_BOOK);
        DOC_SETMARGINS(NDOCID, 0, 200, 15, 40, 15, 1);
        DOC_PRINTLINES(NDOCID, 0, WRITTENTEXT_BOOKSTAND_ALCHEMY_1);
        DOC_PRINTLINES(NDOCID, 0, "");
        DOC_PRINTLINES(NDOCID, 0, WRITTENTEXT_BOOKSTAND_ALCHEMY_2);
        DOC_PRINTLINES(NDOCID, 0, "");
        DOC_PRINTLINES(NDOCID, 0, WRITTENTEXT_BOOKSTAND_ALCHEMY_3);
        DOC_PRINTLINES(NDOCID, 0, "");
        DOC_PRINTLINES(NDOCID, 0, WRITTENTEXT_BOOKSTAND_ALCHEMY_4);
        DOC_SETMARGINS(NDOCID, -(1), 15, 15, 225, 15, 1);
        DOC_PRINTLINES(NDOCID, 1, WRITTENTEXT_BOOKSTAND_ALCHEMY_5);
        DOC_PRINTLINES(NDOCID, 1, "");
        DOC_PRINTLINES(NDOCID, 1, WRITTENTEXT_BOOKSTAND_ALCHEMY_6);
        DOC_PRINTLINES(NDOCID, 1, "");
        DOC_PRINTLINES(NDOCID, 1, WRITTENTEXT_BOOKSTAND_ALCHEMY_7);
        DOC_SHOW(NDOCID);
    };
}

