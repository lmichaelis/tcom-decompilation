func void PLAYER_MOB_MISSING_ITEM() {
    RESERVED_CONST_INT_01 = MEM_READINT(11216516);
    RESERVED_CONST_INT_01 = MEM_READINT((RESERVED_CONST_INT_01) + (OCNPC__FOCUSVOB_OFFSET));
    if (!(HLP_IS_OCMOBINTER(RESERVED_CONST_INT_01))) {
        return;
    };
    RESERVED_CONST_INT_01 = MEM_GETSYMBOLINDEX(MEM_READSTRING((RESERVED_CONST_INT_01) + (420)));
    if ((RESERVED_CONST_INT_01) <= (0)) {
        return;
    };
    RESERVED_CONST_INT_01 = ITM_GETPTR(RESERVED_CONST_INT_01);
    if ((RESERVED_CONST_INT_01) == (0)) {
        return;
    };
    ITEM = _^(RESERVED_CONST_INT_01);
    if ((HLP_GETINSTANCEID(ITEM)) == (36950)) {
        PRINT(PRINT_MISSINGITEM);
    };
    PRINT(CONCATSTRINGS(PRINT_MISSINGITEM, ITEM.NAME));
    AI_PLAYANI(SELF, "T_DONTKNOW");
    B_SAY_OVERLAY(SELF, SELF, "$MISSINGITEM");
}

