func void EQUIP_ITAM_FANEAMULETT() {
    if ((FANE_EQUIPAMULETTE) == (0)) {
        FANE_MAGICLEVEL = SELF.PROTECTION[5];
        FANE_EQUIPAMULETTE = (FANE_EQUIPAMULETTE) + (1);
    };
    if ((FANE_EQUIPAMULETTE) == (1)) {
        SELF.ATTRIBUTE[1] = (SELF.ATTRIBUTE[1]) + (HP_FANEAMULETT);
        if ((SELF.ID) == (0)) {
            MARVINTRANSFORMHP = (MARVINTRANSFORMHP) + (HP_FANEAMULETT);
        };
        SELF.PROTECTION[2] += WEAPONPROT_FANEAMULETT;
        SELF.PROTECTION[1] += WEAPONPROT_FANEAMULETT;
        SELF.PROTECTION[6] += POINTPROT_FANEAMULETT;
        if ((SELF.PROTECTION[5]) >= ((MAGICDEBUFF_FANEAMULETT) * (-(1)))) {
            SELF.PROTECTION[5] += MAGICDEBUFF_FANEAMULETT;
        } else {
            SELF.PROTECTION[5] = 0;
        };
    };
}

func void UNEQUIP_ITAM_FANEAMULETT() {
