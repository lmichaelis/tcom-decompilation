func void B_FREEZEPLAYER() {
    var string CONCATTEXT2;
    var string CONCATTEXT1;
    var int ABZUG;
    var int FREEZEPLAYER_HITPOINTS;
    var int CURRENTHITPOINTS;
    CURRENTHITPOINTS = HERO.ATTRIBUTE[0];
    FREEZEPLAYER_HITPOINTS = ((CURRENTHITPOINTS) * (4)) / (5);
    if ((FREEZEPLAYER_HITPOINTS) < (2)) {
        FREEZEPLAYER_HITPOINTS = 2;
    };
    ABZUG = (HERO.ATTRIBUTE[0]) - (FREEZEPLAYER_HITPOINTS);
    if ((ABZUG) > (0)) {
        CONCATTEXT1 = CONCATSTRINGS(" ", NAME_DAMAGE);
        CONCATTEXT2 = CONCATSTRINGS(INTTOSTRING(ABZUG), CONCATTEXT1);
        WLD_PLAYEFFECT("FX_IceWave", HERO, HERO, 0, 0, 0, FALSE);
        SND_PLAY("MFX_Icewave_Cast");
        AI_PLAYANI(HERO, "T_STUMBLE");
    };
    HERO.ATTRIBUTE[0] = FREEZEPLAYER_HITPOINTS;
}

func void CANTWEARPEACEKEEPER() {
    var string STRMESSAGE;
    STRMESSAGE = PRINT_ADDON_BELIARSCOURSE_MISSING;
    B_FREEZEPLAYER();
    Q514_SWORD_FAILEDTOEQUIP = TRUE;
}

func void EQUIP_PEACEKEEPER() {
    if ((SELF.ID) == (0)) {
        AI_UNEQUIPWEAPONS(HERO);
        CANTWEARPEACEKEEPER();
        SCAV_SEEDS_RANDOMIZER = 1;
    };
}

func void UNEQUIP_PEACEKEEPER() {
    if ((SELF.ID) == (0)) {
        SCAV_SEEDS_RANDOMIZER = 0;
    };
}

