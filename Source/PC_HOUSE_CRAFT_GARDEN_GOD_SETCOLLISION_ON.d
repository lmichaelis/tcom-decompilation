func void PC_HOUSE_CRAFT_GARDEN_GOD_SETCOLLISION_ON() {
    if ((HOUSE_CRAFT_GARDEN_GOD_OPTION_INNOS) == (TRUE)) {
        CHANGEVOBCOLLISION("KM_HEROHOUSE_VOB_GARDEN_GOD_INNOS", TRUE, TRUE, TRUE, TRUE);
    };
    if ((HOUSE_CRAFT_GARDEN_GOD_OPTION_ADANOS) == (TRUE)) {
        CHANGEVOBCOLLISION("KM_HEROHOUSE_VOB_GARDEN_GOD_NORMAL", TRUE, TRUE, TRUE, TRUE);
    };
    if ((HOUSE_CRAFT_GARDEN_GOD_OPTION_BELIAR) == (TRUE)) {
        CHANGEVOBCOLLISION("KM_HEROHOUSE_VOB_GARDEN_GOD_BELIAR", TRUE, TRUE, TRUE, TRUE);
    };
}

func void PC_HOUSE_CRAFT_GARDEN_GOD_SETCOLLISION_OFF() {
    if ((HOUSE_CRAFT_GARDEN_GOD_OPTION_INNOS) == (TRUE)) {
        CHANGEVOBCOLLISION("KM_HEROHOUSE_VOB_GARDEN_GOD_INNOS", FALSE, FALSE, FALSE, TRUE);
    };
    if ((HOUSE_CRAFT_GARDEN_GOD_OPTION_ADANOS) == (TRUE)) {
        CHANGEVOBCOLLISION("KM_HEROHOUSE_VOB_GARDEN_GOD_NORMAL", FALSE, FALSE, FALSE, TRUE);
    };
    if ((HOUSE_CRAFT_GARDEN_GOD_OPTION_BELIAR) == (TRUE)) {
        CHANGEVOBCOLLISION("KM_HEROHOUSE_VOB_GARDEN_GOD_BELIAR", FALSE, FALSE, FALSE, TRUE);
    };
}

func void PC_HOUSE_CRAFT_GARDEN_GOD_CLEARALLVOBS() {
    if ((HOUSE_CRAFT_GARDEN_GOD_OPTION_INNOS) == (TRUE)) {
        PLAYERHOUSE_GOD_INNOS += 1;
        WLD_SENDTRIGGER("KM_HEROHOUSE_MOVER_GARDEN_GOD_INNOS");
    };
    if ((HOUSE_CRAFT_GARDEN_GOD_OPTION_ADANOS) == (TRUE)) {
        PLAYERHOUSE_GOD_ADANOS += 1;
        WLD_SENDTRIGGER("KM_HEROHOUSE_MOVER_GARDEN_GOD_ADANOS");
    };
    if ((HOUSE_CRAFT_GARDEN_GOD_OPTION_BELIAR) == (TRUE)) {
        PLAYERHOUSE_GOD_BELIAR += 1;
        WLD_SENDTRIGGER("KM_HEROHOUSE_MOVER_GARDEN_GOD_BELIAR");
    };
}

